  "scripts": {
    "start": "react-scripts start",
    "build:dev": "generate-robotstxt --config=$INIT_CWD/robots-txt.config.dev.js public/robots.txt && react-scripts build",
    "build:prod": "generate-robotstxt --config=$INIT_CWD/robots-txt.config.js public/robots.txt && GENERATE_SOURCEMAP=false react-scripts build",
    "winBuild:dev": "generate-robotstxt --config %INIT_CWD%\\robots-txt.config.dev.js public/robots.txt && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write src",
    "prettifyCheck": "prettier --check src",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "devDependencies": {
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "^27.5.2",
    "@types/lodash.debounce": "4.0.6",
    "@types/lodash.throttle": "4.1.6",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "7.1.25",
    "@types/react-router-dom": "5.1.7",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.11",
    "@types/webappsec-credential-management": "0.6.2",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.18.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest-styled-components": "7.0.4",
    "lint-staged": "10.3.0",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "serve": "12.0.0",
    "shelljs": "0.8.4",
    "source-map-explorer": "2.5.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
